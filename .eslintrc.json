{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "module": false,
    "__DEV__": false,
    "DEBUG": false,
    "process": false
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "rules": {
    "no-debugger": 2,
    "linebreak-style": 0
  }
}