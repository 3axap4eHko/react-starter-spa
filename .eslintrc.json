{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true
  },
  "globals": {
    "module": false,
    "__DEV__": false,
    "DEBUG": false,
    "process": false
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "rules": {
    "react/prop-types": 0,
    "react/no-children-prop": 0,
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0,
    "react/jsx-filename-extension": 0,
    "import/extensions": 0,
    "import/no-named-as-default-member": 0
  }
}